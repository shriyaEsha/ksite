<script>
$(document).ready(function(){
$(".navbar-toggle").click(function(){
  closeall();
  
});

$(".bucketCircle").click(function(){
    
    popOut($(this));
    $(".left").delay(800).animate({'marginLeft':"0px"},800,'easeOutSine');
  });
$(".close").click(function(){
  closeall();
});

function popOut(thisele){
  $(thisele).addClass("popOutFast");
  $(".bucketCircle").each(function(i){
      var ele = $(this);
      setTimeout(function(){ if(!$(ele).hasClass("popOut")) $(ele).addClass("popOut"); }, i*75);
  });
$("html,body").delay(800).animate({'scrollTop':"20px"},1000);
}
function popIn(){

  $(".bucketCircle").each(function(i){
      var ele = $(this);
      setTimeout(function(){ $(ele).removeClass("popOut").removeClass("popOutFast");
       }, i*70);
  });
}

function closeall(){
    $(".left").animate({'marginLeft':"100%"},1000,'easeOutElastic');
    popIn();
}


});
  
</script>

<!--navbar script-->
<script>
$(document).ready(function(){
$(".navbar-toggle").click(function(){
  $(".top-bar").toggleClass("top-bar-anim");
  $(".middle-bar").toggleClass("middle-bar-anim");
  $(".bottom-bar").toggleClass("bottom-bar-anim");
});


if( $(window).width() >= 600)
{
  $(".eventHeader").animate({'opacity':"0"},1000,'easeOutElastic').fadeOut();
  $(".loginHeader").animate({'opacity':"0"},1000,'easeOutElastic').fadeOut();

  $("#eventsNav").hover(function(){
  $(".eventHeader").animate({'opacity':"1"},100,'easeOutElastic').fadeIn();
  });
  $("#loginNav").hover(function(){
  $(".loginHeader").animate({'opacity':"1"},100,'easeOutElastic').fadeIn();
  });

  $(".navbar-nav li").hover(function(){
    if($(this).attr("id") != "eventsNav" )
  {
  $(".eventHeader").animate({'opacity':"0"});
  $(".eventHeader").fadeOut().finish();
  }
    if($(this).attr("id") != "wkshopsNav" )
  {
  $(".wkshopHeader").animate({'opacity':"0"});
  $(".wkshopHeader").fadeOut().finish();
  }

    if($(this).attr("id") != "loginNav" )
  {
  $(".loginHeader").animate({'opacity':"0"});
  $(".loginHeader").fadeOut().finish();
  }
  });

  $(".eventHeader").hover(function(){
  }, function(){
   $(".eventHeader").animate({'opacity':"0"}).fadeOut();
  });

  $(".wkshopHeader").hover(function(){
  }, function(){
   $(".wkshopHeader").animate({'opacity':"0"}).fadeOut();
  });

  $(".loginHeader").hover(function(){
  }, function(){
   $(".loginHeader").animate({'opacity':"0"}).fadeOut();
  });
  
  $(".wkshopHeader").animate({'opacity':"0"}).fadeOut().finish();
  $("#wkshopsNav").hover(function(){
    $(".wkshopHeader").animate({'opacity':"1"},100,'easeOutElastic').fadeIn();
  });
}

});
</script>
